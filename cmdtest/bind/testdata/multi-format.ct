# Multi-format: comma input
$ argonaut bind --shell-type=sh --flag=tags --flag-tags-multi --flag-tags-multi-format=comma -- a --tags=one,two,three
TAGS='one,two,three'

# Multi-format: space separated (simulate quoting as necessary)
$ argonaut bind --shell-type=sh --flag=tags --flag-tags-multi --flag-tags-multi-format=space -- a --tags="one two three"
TAGS='"one'

# Multi-format: newline style (passed via repeated flags)
$ argonaut bind --shell-type=sh --flag=tags --flag-tags-multi --flag-tags-multi-format=newline -- a --tags=one --tags=two --tags=three
TAGS='one
two
three'

# Multi-format: json array input
$ argonaut bind --shell-type=sh --flag=tags --flag-tags-multi --flag-tags-multi-format=json -- a --tags='["one","two"]'
TAGS='["'\''[\"one\",\"two\"]'\''"]'
