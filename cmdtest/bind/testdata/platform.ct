# Platform: explicit shell-type sh
$ argonaut bind --shell-type=sh --flag=name --flag-name-env-name=NAME -- a --name=alice
NAME='alice'

# Platform: explicit shell-type powershell
$ argonaut bind --shell-type=powershell --flag=name --flag-name-env-name=NAME -- a --name=alice
$Env:NAME = 'alice'

# Platform: explicit shell-type cmd (windows)
$ argonaut bind --shell-type=cmd --flag=name --flag-name-env-name=NAME -- a --name=alice
set "NAME=alice"
